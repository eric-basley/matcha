{
  "name": "matcha",
  "version": "0.0.1",
  "description": "projet matcha 42",
  "main": "index.js",
  "repository": "https://github.com/abarriel/matcha.git",
  "author": "abarriel <abarriel@student.42.fr>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src --color",
    "srv:dev": "cross-env NODE_ENV=development DEBUG=matcha* nodemon --watch config/server --watch src/server --exec \"babel-node src/server\"",
    "srv:prod": "cross-env NODE_ENV=production babel src/server src/lib --out-dir dist/",
    "srv:run": "cross-env NODE_ENV=production DEBUG=sfs*,evtx* node dist/server/cluster",
    "load:data": "cross-env NODE_ENV=development DEBUG=matcha* babel-node ./scripts/load_data",
    "reset:data": "cross-env NODE_ENV=testing DEBUG=matcha* babel-node ./scripts/reset_data",
    "srv:test": "cross-env NODE_ENV=testing DEBUG=matcha* mocha --require babel-core/register --reporter spec \"./src/server/**/__test__/*.js\" ",
    "client:test": "cross-env NODE_ENV=testing mocha --require ignore-styles --require babel-core/register --reporter spec \"./src/client/**/__test__/*.js\" ",
    "coverage": "cross-env NODE_ENV=testing nyc yarn test",
    "build:prod": "cross-env NODE_ENV=production webpack -p --config webpack.config.js --progress",
    "build:dev": "cross-env NODE_ENV=development webpack-dev-server --progress --config webpack.config.js",
    "bundle:stats": "cross-env NODE_ENV=production webpack -p --config webpack.config.js --profile --json > stats.json",
    "bundle:analyse": "webpack-bundle-analyzer ./stats.json ./build --report --mode=static"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "bcrypt-as-promised": "^1.1.0",
    "body-parser": "^1.17.2",
    "compression": "^1.6.2",
    "debug": "^2.6.8",
    "evtx": "^0.3.2",
    "express": "^4.15.3",
    "geoip-lite": "^1.2.1",
    "geolib": "^2.0.22",
    "joi": "^10.6.0",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "morgan": "^1.8.2",
    "morgan-debug": "^2.0.0",
    "nodemailer": "^4.0.1",
    "pg-promise": "^5.9.3",
    "ramda": "^0.24.1",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "reselect": "^3.0.1",
    "socket.io": "^1.0.0",
    "socket.io-client": "^2.0.3",
    "universal-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.2.8",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "cross-env": "^3.2.4",
    "css-loader": "^0.26.1",
    "enzyme": "^2.6.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "ignore-styles": "^5.0.1",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.2",
    "sinon": "^2.3.4",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.1.1",
    "should": "^11.2.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-bundle-analyzer": "^2.3.0",
    "webpack-dev-server": "^2.4.1"
  },
  "nyc": {
    "exclude": [
      "build"
    ]
  },
  "babel": {
    "presets": [
      [
        "latest",
        {
          "modules": false
        }
      ],
      "stage-0",
      "react"
    ]
  }
}
